<html>
    <head>
        <title>Terraform'd VMs - OpenMRS community infrastructure</title>
    </head>
    <body>
        <h1>OpenMRS Community VMs</h1>
        <p>(autogenerated from terraform)</p>
        <% $vms.each do |vm, info| %>
          <h3><%= vm %></h3>
          <ul>
          <% info.each do |key,value| %>
            <% if value.is_a?(Array) %>
              <li>
                <b><%= key %></b>:
                <ul>
                <% value.each do |item| %>
                  <li><%= item %></li>
                <% end %>
                </ul>
              </li>
            <% else %>
              <li><b><%= key %></b>: <%= value %></li>
            <% end %>
          <% end %>
        </ul>
        <% end %>
        <a href="https://jetstream-cloud.org/tech-specs/cloud-services.php">Sizes Documentation</a>
    </body>
</html>
